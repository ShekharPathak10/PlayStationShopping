<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>src.TestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetConsoleServlet</servlet-name>
        <servlet-class>GetConsoleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetConsolesServlet</servlet-name>
        <servlet-class>src.GetConsolesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddConsoleServlet</servlet-name>
        <servlet-class>src.AddConsoleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveConsoleServlet</servlet-name>
        <servlet-class>src.RemoveConsoleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidateConsoleServlet</servlet-name>
        <servlet-class>src.ValidateConsoleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/TestServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetConsoleServlet</servlet-name>
        <url-pattern>/GetConsoleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetConsolesServlet</servlet-name>
        <url-pattern>/GetConsolesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddConsoleServlet</servlet-name>
        <url-pattern>/AddConsoleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveConsoleServlet</servlet-name>
        <url-pattern>/RemoveConsoleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidateConsoleServlet</servlet-name>
        <url-pattern>/ValidateConsoleServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
